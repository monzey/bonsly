<keymap version="1" name="Vim" parent="$default">
  <action id="ActivateProjectToolWindow">
    <keyboard-shortcut first-keystroke="alt 1" />
    <keyboard-shortcut first-keystroke="alt comma" second-keystroke="alt q" />
  </action>
  <action id="ActivateStructureToolWindow">
    <keyboard-shortcut first-keystroke="alt 7" />
    <keyboard-shortcut first-keystroke="alt comma" second-keystroke="shift alt comma" />
  </action>
  <action id="Back">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
    <mouse-shortcut keystroke="button4" />
    <keyboard-shortcut first-keystroke="alt comma" second-keystroke="alt comma" />
  </action>
  <action id="ChooseDebugConfiguration">
    <keyboard-shortcut first-keystroke="shift alt f9" />
    <keyboard-shortcut first-keystroke="alt comma" second-keystroke="alt d" />
  </action>
  <action id="ChooseRunConfiguration">
    <keyboard-shortcut first-keystroke="shift alt f10" />
    <keyboard-shortcut first-keystroke="alt comma" second-keystroke="alt r" />
  </action>
  <action id="CloseAllUnmodifiedEditors">
    <keyboard-shortcut first-keystroke="alt g" second-keystroke="alt x" />
  </action>
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl f4" />
    <keyboard-shortcut first-keystroke="alt g" second-keystroke="alt d" />
  </action>
  <action id="CommentByLineComment">
    <keyboard-shortcut first-keystroke="ctrl slash" />
    <keyboard-shortcut first-keystroke="ctrl divide" />
    <keyboard-shortcut first-keystroke="alt comma" second-keystroke="alt c" />
  </action>
  <action id="Debug">
    <keyboard-shortcut first-keystroke="shift f9" />
    <keyboard-shortcut first-keystroke="alt comma" second-keystroke="shift alt d" />
  </action>
  <action id="Forward">
    <keyboard-shortcut first-keystroke="ctrl alt right" />
    <mouse-shortcut keystroke="button5" />
    <keyboard-shortcut first-keystroke="alt comma" second-keystroke="alt semicolon" />
  </action>
  <action id="GotoDeclaration">
    <keyboard-shortcut first-keystroke="ctrl b" />
    <mouse-shortcut keystroke="control button1" />
    <mouse-shortcut keystroke="button2" />
    <keyboard-shortcut first-keystroke="alt comma" second-keystroke="alt enter" />
  </action>
  <action id="MethodDown">
    <keyboard-shortcut first-keystroke="alt down" />
    <keyboard-shortcut first-keystroke="alt n" />
  </action>
  <action id="MethodUp">
    <keyboard-shortcut first-keystroke="alt up" />
    <keyboard-shortcut first-keystroke="alt p" />
  </action>
  <action id="NextSplitter">
    <keyboard-shortcut first-keystroke="alt s" second-keystroke="alt j" />
  </action>
  <action id="NextTab">
    <keyboard-shortcut first-keystroke="alt right" />
    <keyboard-shortcut first-keystroke="shift alt k" />
  </action>
  <action id="PrevSplitter">
    <keyboard-shortcut first-keystroke="alt s" second-keystroke="alt k" />
  </action>
  <action id="PreviousTab">
    <keyboard-shortcut first-keystroke="alt left" />
    <keyboard-shortcut first-keystroke="shift alt j" />
  </action>
  <action id="SearchEverywhere">
    <keyboard-shortcut first-keystroke="alt comma" second-keystroke="alt p" />
  </action>
  <action id="SelectNextOccurrence" />
  <action id="SplitHorizontally">
    <keyboard-shortcut first-keystroke="alt comma" second-keystroke="alt s" />
  </action>
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="alt comma" second-keystroke="alt v" />
  </action>
  <action id="TableResult.UnselectPreviousOccurrence" />
  <action id="ToggleLineBreakpoint">
    <keyboard-shortcut first-keystroke="ctrl f8" />
    <keyboard-shortcut first-keystroke="alt comma" second-keystroke="alt b" />
  </action>
  <action id="UnselectPreviousOccurrence" />
  <action id="Unsplit">
    <keyboard-shortcut first-keystroke="alt s" second-keystroke="alt d" />
  </action>
  <action id="VimInsertExitMode">
    <keyboard-shortcut first-keystroke="alt j" />
  </action>
</keymap>